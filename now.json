{
  "version": 2,
  "alias": ["www.jeremyswinnen.com", "jeremyswinnen.com"],
  "builds": [
    { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "www" } }
  ],
  "routes": [
	  { "handle": "filesystem" },
	  { "src": "/.*", "status": 404, "dest": "http://www.jeremyswinnen.com/404.html" }
	]
}